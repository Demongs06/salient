{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/single-service",
	"version": "0.1.0",
	"title": "Single Service",
	"category": "widgets",
	"icon": "embed-photo",
	"description": "FSPL offers these services, add inside of Services block.",
	"example": {},
	"supports": {
		"html": false,
		"align": true
	},
	"attributes": {
		"title": {
			"type": "string",
			"selector": "h2.title",
			"source": "html"
		},
		"info": {
			"type": "string",
			"selector": "p.info",
			"source": "html",
			"align": [ "left", "right", "full" ]
		},
		"alt": {
            "type": "string",
            "source": "attribute",
            "selector": "img",
            "attribute": "alt",
            "default": ""
        },
        "url": {
            "type": "string",
            "source": "attribute",
            "selector": "img",
            "attribute": "src"
        },
        "id": {
            "type": "number"
        }
	},
	"textdomain": "blox-site",
	"editorScript": "file:./index.js",
	"editorStyle": [ "file:../block.css", "file:./index.css" ],
	"style": [ "file:./style-index.css", "file:../block.css" ],
	"viewScript": "file:./view.js"
}

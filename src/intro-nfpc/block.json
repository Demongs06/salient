{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/intro-nfpc",
	"version": "0.1.0",
	"title": "Home page intro block",
	"category": "widgets",
	"icon": "embed-photo",
	"description": "Intro Block with image, only for the home page.",
	"example": {},
	"supports": {
		"html": true,
		"block": true,
		"recover": true,
		"spacing": {
			"padding": true,
			"margin": true
		}
	},
	"attributes": {
		"title": {
			"type": "string",
			"selector": "h1",
			"source": "html"
		},
		"subtitle": {
			"type": "string",
			"selector": "p",
			"source": "html"
		},
		"id": {
			"type": "number"
		},
		"alt": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "alt",
			"default": ""
		},
		"url": {
			"type": "string",
			"source": "attribute",
			"selector": "img",
			"attribute": "src"
		},
		"paddingTop": {
			"type": "string",
			"default": "0"
		},
		"paddingRight": {
			"type": "string",
			"default": "0"
		},
		"paddingBottom": {
			"type": "string",
			"default": "0"
		},
		"paddingLeft": {
			"type": "string",
			"default": "0"
		}
	},
	"textdomain": "blox-site",
	"editorScript": "file:./index.js",
	"editorStyle": [
		"file:../block.css",
		"file:./index.css"
	],
	"style": [
		"file:./style-index.css",
		"file:../block.css"
	],
	"viewScript": "file:./view.js"
}